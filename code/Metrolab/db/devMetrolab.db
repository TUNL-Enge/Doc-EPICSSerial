record(stringin, "$(P)$(R)NMR")
{
    field(DESC, "NMR Display Value")
    field(DTYP, "Metrolab")
    field(INP,  "#L$(L) A$(A) @0")
    field(PINI, "YES")
}

record(bo, "$(P)$(R)Remote")
{
    field(DESC, "Remote mode")
    field(DTYP, "Metrolab")
    field(OUT,  "#L$(L) A$(A) @1")
}

record(bo, "$(P)$(R)Local")
{
    field(DESC, "Local mode")
    field(DTYP, "Metrolab")
    field(OUT,  "#L$(L) A$(A) @2")
}

record(stringin, "$(P)$(R)Field")
{
    field(DESC, "Get the field value")
    field(DTYP, "Metrolab")
    field(SCAN, ".2 second")
    field(INP,  "#L$(L) A$(A) @3")
}

